{
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "containerDefinitions": [
      {
        "name": "portfolio",
        "image": "latest",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 7001,
            "containerPort": "7001",
            "protocol": "tcp"
          }
        ]
      }
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "memory": "1",
    "cpu": "1",
    "executionRoleArn": "ecsTaskExecutionRole",
    "family": "portfolio-task-definition8",
    "networkMode": "awsvpc"
  }
  
  