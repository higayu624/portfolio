{
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "containerDefinitions": [
      {
        "name": "portfolioGo-container8",
        "image": "latest",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 7001,
            "containerPort": "7001",
            "protocol": "tcp"
          }
        ]
      }
    ],
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "ecsTaskExecutionRole",
  }
  
  